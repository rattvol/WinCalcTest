<?xml version="1.0" encoding="utf-8" ?>
<operations>
    <operation>
      <!--Первое число: целое от 0 до 1 000 000 000-->
      <firstNum>2</firstNum>
      <!--Второе число: целое от 0 до 1 000 000 000-->
      <secondNum>2</secondNum>
      <!--Операции над числами: 'плюс', 'минус', 'умножить', 'разделить'-->
      <function>плюс</function>
      <!--Ожидаемый результат вычисления-->
      <result>4</result>
      <!--Для дробных результатов деления использовать "," для отделения десятичной части-->
  </operation>
  <operation>
    <firstNum>159</firstNum>
    <secondNum>32</secondNum>
    <function>умножить</function>
    <result>11200</result>
  </operation>
  <operation>
    <firstNum>10</firstNum>
    <secondNum>10</secondNum>
    <function>минус</function>
    <result>0</result>
  </operation>
  <operation>
    <firstNum>15</firstNum>
    <secondNum>32</secondNum>
    <function>плюс</function>
    <result>47</result>
  </operation>
</operations>
